<?xml version="1.0" encoding="UTF-8"?>

<?import intransix.osm.termite.gui.property.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="javafx.scene.layout.VBox" alignment="CENTER" spacing="5.0" xmlns:fx="http://javafx.com/fxml">
  <children>
    <ScrollPane id="ScrollPane" fitToHeight="true" fitToWidth="true" prefViewportHeight="400.0" prefViewportWidth="248.0">
      <content>
        <PropertyTable fx:id="propertyTable" />
      </content>
    </ScrollPane>
    <HBox id="HBox" alignment="CENTER" spacing="20.0">
      <children>
        <Button id="editButton" fx:id="newButton" mnemonicParsing="false" onAction="#onAddButton" text="New" />
        <Button fx:id="editButton" mnemonicParsing="false" text="Edit" />
        <Button fx:id="deleteButton" mnemonicParsing="false" text="Delete" />
      </children>
      <padding>
        <Insets bottom="10.0" top="10.0" />
      </padding>
    </HBox>
  </children>
</fx:root>
